  <conditions>                                                                
  <!-- conditions are dependency rules that can apply to a component or an individual file -->
    <condition id="ADC">
        <description>IF ADC1 is selected, ADC2 and BDRV must also be selected</description>
        <require Cclass="Device" Cgroup="SDK" Csub="ADC1"/>
        <require Cclass="Device" Cgroup="SDK" Csub="ADC2"/>
        <require condition="TLE987x CMSIS Device"/>
    </condition>
    
    <condition id="Only ConfigWizard">
    <!-- Condition for ConfigWizard_v1.x -->
        <description>If ConfigWizard_v1.x is selected, version 2.x must not be selected</description>
        <deny Cclass="Device" Cgroup="ConfigWizard_v2" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987*QX*"/>
    </condition>
    
    <condition id="Only ConfigWizard_v2">
    <!-- Condition for ConfigWizard_v2.x -->
        <description>If ConfigWizard_v2.x is selected, version 1.x must not be selected</description>
        <deny Cclass="Device" Cgroup="ConfigWizard" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987*QX*"/>
    </condition>
    
    <condition id="TLE987x CMSIS Device">                                     
    <!-- conditions selecting Devices -->
      <description>Infineon TLE987x device series</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Cclass="Device" Cgroup="SDK" Csub="SCU"/>
      <require Cclass="Device" Cgroup="SDK" Csub="BOOTROM"/>
      <require Cclass="Device" Cgroup="SDK" Csub="WDT1"/>
      <require Cclass="Device" Cgroup="SDK" Csub="ISR"/>
      <require Cclass="Device" Cgroup="SDK" Csub="INT"/>
      <require Cclass="Device" Cgroup="SDK" Csub="LIN"/>
      <require Cclass="Device" Cgroup="SDK" Csub="PORT"/>
      <require Cclass="Device" Cgroup="SDK" Csub="PMU"/>
      <require Dvendor="Infineon:7" Dname="TLE987*QX*"/>
      <require Tcompiler="ARMCC"/>
      <accept Cclass="Device" Cgroup="ConfigWizard" Csub=""/>
      <accept Cclass="Device" Cgroup="ConfigWizard_v2" Csub=""/>
    </condition>
<!--
    <condition id="TLE987x">
      <description>Infineon TLE987x device series</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987*QX*"/>
      <require Tcompiler="ARMCC"/>
    </condition>
-->
    <condition id="TLE987x no -2QXA40">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE987x device series</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987?QX*"/> <!-- "?" is the Wild-card for a single character; this excludes TLE9879-2QXA40 -->
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="TLE987x_20">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE987x 24MHz device series</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987*QX*20"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="TLE987x_40">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE987x 40MHz device series</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987*QX*40"/> <!-- TLE9879-2QXA40 is included -->
      <require Tcompiler="ARMCC"/>
    </condition>
    
    <condition id="TLE987x_40 no -2QXA40">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE987x 40MHz device series</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE987?QX*40"/> <!-- "?" is the Wild-card for a single character; this excludes TLE9879-2QXA40 -->
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="TLE9879_2QXA40">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE9879-2QXA40 device</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9879-2QXA40"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="TLE9879_2QXA40_ADC34">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE9879-2QXA40 device</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <require condition="TLE9879_2QXA40"/>
      <require Cclass="Device" Cgroup="SDK" Csub="ADC1"/>
      <require Dvendor="Infineon:7" Dname="TLE9879-2QXA40"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="TLE987x_BF">
      <!-- conditions selecting Devices -->
      <description>Infineon TLE987x device series, BF-Step</description>
      <require Cclass ="CMSIS" Cgroup="CORE" Csub=""/>
      <accept condition="TLE9871QXA"/>
      <accept condition="TLE9877QXA"/>
      <accept condition="TLE9879QXA"/>
      <require Tcompiler="ARMCC"/>
    </condition>

